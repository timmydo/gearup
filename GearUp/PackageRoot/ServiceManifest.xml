<?xml version="1.0" encoding="utf-8"?>
<ServiceManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema"
				 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 Name="FE" Version="1.0.0"
				 xmlns="http://schemas.microsoft.com/2011/01/fabric">
	<ServiceTypes>
		<StatelessServiceType ServiceTypeName="FEType" />
	</ServiceTypes>
	<CodePackage Name="C" Version="1.0.0">
		<EntryPoint>
			<ExeHost>
				<Program>approot\run.cmd</Program>
				<WorkingFolder>CodePackage</WorkingFolder>
				<ConsoleRedirection FileRetentionCount="5" FileMaxSizeInKb="2048"/>
			</ExeHost>
		</EntryPoint>
	</CodePackage>
	<Resources>
		<Endpoints>
			<Endpoint Name="FEEndpoint" Protocol="http" Type="Input" Port="8088"/>
		</Endpoints>
	</Resources>
</ServiceManifest>